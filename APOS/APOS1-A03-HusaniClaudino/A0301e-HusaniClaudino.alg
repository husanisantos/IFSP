algoritmo "ParOuImpar"
// Autor : Husani Santos
// Data : 01/07/2021
// Seção de Declarações 
var
    num : inteiro

inicio
// Seção de Comandos 
   num <- 20
   SE num % 2 = 0 ENTAO
      ESCREVAL(num," é um número par")
   SENAO
      ESCREVAL(num," é um número ímpar")
   FIMSE
      
fimalgoritmo
